{
  "name": "the-node-skeleton",
  "version": "1.1.0",
  "description": "The Node skeleton",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && echo 'Node environment set to development' && nodemon index.js && exit 0",
    "start": "(pm2 stop 'the-node-skeleton' || echo 'Nothing to kill') && pm2 start index.js --name 'the-node-skeleton'"
  },
  "keywords": [
    "Node",
    "Skeleton",
    "ES6"
  ],
  "author": "praemineo",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.1",
    "bunyan-format": "^0.2.1",
    "bunyan-slack": "0.0.10",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dtrace-provider": "^0.8.7",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "mongoose": "^5.4.7",
    "response-time": "^2.3.2",
    "socket.io": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1"
  }
}
