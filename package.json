{
  "name": "the-node-skeleton",
  "version": "1.1.0",
  "description": "The Node skeleton",
  "main": "index.js",
  "scripts": {
    "dev": "export NODE_ENV=development && echo 'Node environment set to development' && nodemon index.js && exit 0",
    "start": "(pm2 stop 'the-node-skeleton' || echo 'Nothing to kill') && pm2 start index.js --name 'the-node-skeleton'",
  },
  "keywords": [
    "Node",
    "Skeleton",
    "ES6"
  ],
  "author": "preeminece",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.1",
    "bunyan-format": "^0.2.1",
    "bunyan-slack": "0.0.10",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "helmet": "^2.1.3",
    "mongoose": "^4.11.6",
    "response-time": "^2.3.2",
    "socket.io": "^2.0.3",
    "socketio": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^7.0.1",
    "eslint-plugin-import": "^1.14.0"
  }
}
